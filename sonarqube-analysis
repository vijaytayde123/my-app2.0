node{
     stage('Scm Checkout'){  
        git credentialsId: 'github-cred', url: 'https://github.com/vijaytayde123/my-app' 
     }
      stage('Mvn Package'){
	   sh label: '', script: 'mvn compile'
	 }
      stage('SonarQube Analysis') {
        withSonarQubeEnv('SonarQube') { 
		   sh "/bin/mvn sonar:sonar"
        } 
}
